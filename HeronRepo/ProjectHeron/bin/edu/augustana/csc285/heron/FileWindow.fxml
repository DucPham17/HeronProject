<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="324.0" prefWidth="434.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.augustana.csc285.heron.FileWindowController">
   <children>
      <Button fx:id="BrowseBtn" layoutX="217.0" layoutY="253.0" mnemonicParsing="false" onAction="#handleBrowse" prefHeight="25.0" prefWidth="88.0" text="New Project" />
      <Button fx:id="NextBtn" layoutX="358.0" layoutY="278.0" mnemonicParsing="false" onAction="#handleNext" prefHeight="23.0" prefWidth="54.0" text="Next" />
      <Button fx:id="loadBtn" layoutX="135.0" layoutY="253.0" mnemonicParsing="false" onAction="#handleLoad" text="Load Project" />
      <TextField fx:id="fileField" layoutX="70.0" layoutY="228.0" prefHeight="25.0" prefWidth="311.0" />
      <Button fx:id="aboutBtn" layoutX="21.0" layoutY="278.0" mnemonicParsing="false" onAction="#handleAbout" text="About" />
      <ImageView fx:id="imageView" fitHeight="177.0" fitWidth="125.0" layoutX="256.0" layoutY="51.0" pickOnBounds="true" preserveRatio="true" />
   </children>
</AnchorPane>
